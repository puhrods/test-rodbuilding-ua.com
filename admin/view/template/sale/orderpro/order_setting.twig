{{ header }}{{ column_left }}
<div id="content" class="setting-content">
	<div class="page-header">
		<div class="container-fluid">
		  <div class="pull-right">
			<div class="buttons">
				<a onclick="$('#setting_form').submit();" class="btn btn-success btn-sm">{{ button_save }}</a>
				<a href="{{ cancel }}" class="btn btn-danger btn-sm">{{ button_cancel }}</a>
			</div>
		  </div>
		  <h1>{{ heading_title }}</h1>
		  <ul class="breadcrumb">
			{% for breadcrumb in breadcrumbs %}
			<li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
			{% endfor %}
		  </ul>
		</div>
	</div>
	<div id="notifications"></div>
	<div class="container-fluid setting-form">
	{% if error_license %}
		<div class="alert alert-danger"><i class="fa fa-check-circle"></i> {{ error_license }}</div>
	{% endif %}
	{% if bom_files %}
		<div class="alert alert-warning">
			<div><i class="fa fa-check-circle"></i> {{ error_bom }}</div>
			{% for bom_file in bom_files %}
				<div class="text-danger">{{ bom_file }}</div>
			{% endfor %}
		</div>
	{% endif %}
